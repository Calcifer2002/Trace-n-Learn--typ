<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/UI_background"
    tools:context=".A_Activity">


    <com.yp.tracenlearn.ACustomView
        android:id="@+id/customACanvas"
        android:layout_width="204dp"
        android:layout_height="257dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.388" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="335dp"
        android:layout_height="743dp"
        app:srcCompat="@drawable/Tree2"
        tools:layout_editor_absoluteX="-160dp"
        tools:layout_editor_absoluteY="221dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="437dp"
        android:layout_height="852dp"
        app:srcCompat="@drawable/Grass1"
        tools:layout_editor_absoluteX="-32dp"
        tools:layout_editor_absoluteY="321dp" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="191dp"
        android:layout_height="417dp"
        app:srcCompat="@drawable/Moose"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="497dp" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="169dp"
        android:layout_height="249dp"
        android:rotation="5"
        app:srcCompat="@drawable/Speech_b"
        tools:layout_editor_absoluteX="208dp"
        tools:layout_editor_absoluteY="475dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Trace and Learn \n the letter A!"
        android:textColor="@color/black"
        android:textSize="47px"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="235dp"
        tools:layout_editor_absoluteY="573dp" />

    <include
        android:id="@+id/colorPanel"
        layout="@layout/colour_panel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/customACanvas"
        app:layout_constraintBottom_toTopOf="@+id/customACanvas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.754" />

</androidx.constraintlayout.widget.ConstraintLayout>
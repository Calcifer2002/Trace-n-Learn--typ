<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserActivity"
    android:background="@drawable/UI_background">

    <EditText
        android:id="@+id/editName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Enter a username"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.305" />

    <Button
        android:id="@+id/sendUserButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Confirm"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/editName"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="@+id/editName"
        app:layout_constraintTop_toBottomOf="@+id/editName"
        app:layout_constraintVertical_bias="0.088" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="463dp"
        android:layout_height="905dp"
        app:srcCompat="@drawable/Tree2"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="144dp" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/Tree2"
        tools:layout_editor_absoluteX="-187dp"
        tools:layout_editor_absoluteY="185dp" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="419dp"
        android:layout_height="904dp"
        app:srcCompat="@drawable/Grass1"
        tools:layout_editor_absoluteX="-16dp"
        tools:layout_editor_absoluteY="297dp" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="240dp"
        android:layout_height="555dp"
        app:srcCompat="@drawable/Clouds1"
        tools:layout_editor_absoluteX="-26dp"
        tools:layout_editor_absoluteY="-188dp" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="236dp"
        android:layout_height="313dp"
        app:srcCompat="@drawable/Clouds2"
        tools:layout_editor_absoluteX="181dp"
        tools:layout_editor_absoluteY="-128dp" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="131dp"
        android:layout_height="242dp"
        app:srcCompat="@drawable/Clouds3"
        tools:layout_editor_absoluteX="205dp"
        tools:layout_editor_absoluteY="5dp" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="211dp"
        android:layout_height="278dp"
        app:srcCompat="@drawable/Moose"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="559dp" />


</androidx.constraintlayout.widget.ConstraintLayout>